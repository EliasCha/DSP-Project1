  3.719893177067865*^9}},ExpressionUUID->"bcabba0f-a087-477e-b732-\
cc763032fa80"],

Cell[BoxData[{
 RowBox[{"ClearAll", "[", "\"\<`*\>\"", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f", "=", 
    RowBox[{"900", "*", 
     SuperscriptBox["10", "6"]}]}], ";"}], " ", 
  RowBox[{"(*", "Frequency", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"c", "=", "299792458"}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{"Speed", " ", "of", " ", "light"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Omega]", "=", 
   RowBox[{"2", "*", "Pi", "*", "f"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"th", "=", "30"}], ";", " ", 
   RowBox[{"(*", 
    RowBox[{"Tower", " ", "height"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"ch", "=", "1.5"}], ";", " ", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Height", " ", "of", " ", "the", " ", "car"}], "-", "antenna"}], 
    "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Pythagoran", " ", "theorem"}], "*)"}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Direct", " ", "from", " ", "tower", " ", "to", " ", 
     RowBox[{"car", ":"}]}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"distance1", "[", "x_", "]"}], ":=", 
    SqrtBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"th", "-", "ch"}], ")"}], "2"], "+", 
      SuperscriptBox["x", "2"]}]]}], ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"Reflected", " ", "from", " ", "tower", " ", "to", " ", 
    RowBox[{"car", ":"}]}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"distance2", "[", "x_", "]"}], ":=", 
    SqrtBox[
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"th", "+", "ch"}], ")"}], "2"], "+", 
      SuperscriptBox["x", "2"]}]]}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Time", " ", "taken", " ", "for", " ", "the", " ", "signal", " ", "to", 
    " ", "reach", " ", "the", " ", "car", " ", "at", " ", "both", " ", 
    "signals"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"time1", "[", "x_", "]"}], "=", 
   FractionBox[
    RowBox[{"distance1", "[", "x", "]"}], "c"]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"time2", "[", "x_", "]"}], "=", " ", 
    FractionBox[
     RowBox[{"distance2", "[", "x", "]"}], "c"]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"Direct", " ", "signal"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"signal1", "[", "t_", "]"}], ":=", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"\[ImaginaryJ]", 
      RowBox[{"(", 
       RowBox[{"\[Omega]", "*", "t"}], ")"}]}]]}], ";"}], " ", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Reflected", " ", "signal"}], ",", " ", 
    RowBox[{"Phase", " ", "shifted", " ", "direct", " ", "signal"}]}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"signal2", "[", "t_", "]"}], ":=", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"\[ImaginaryJ]", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Omega]", "*", "t"}], "+", "Pi"}], ")"}]}]]}], ";"}], " ", 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Recieved", " ", "signal"}], ",", " ", 
    RowBox[{"from", " ", "assignment"}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"recieved", "[", 
    RowBox[{"t_", ",", " ", "x_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"signal1", "[", 
     RowBox[{"t", "-", 
      RowBox[{"time1", "[", "x", "]"}]}], "]"}], "+", 
    RowBox[{"signal2", "[", 
     RowBox[{"t", "-", 
      RowBox[{"time2", "[", "x", "]"}]}], "]"}]}]}], " ", ";"}]}], "Input",
 CellChangeTimes->{{3.719911243126717*^9, 3.719911268437461*^9}, {
   3.7199113609172*^9, 3.719911361654723*^9}, {3.719911426625291*^9, 
